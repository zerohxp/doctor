.select_area{
    position:fixed;
    bottom:0;
    left:0;
    height:320px;
    width:100%;
    z-index:9999;
    background-color:#FFFFFF;
}
.select_area picker-view-column{
    text-align:center;
}
.select_area .area_button{
    font-size:14px;
    color:#919191;
    margin-top:6px;
    position:relative;
    padding:10px;
    display:inline-block;
    width:100%;
    font-weight:600;
}
.select_area .area_button span:last-child{
    float:right;
    margin-right:20px;
    color:#39c32b;
}
.select_work{
    position:fixed;
    bottom:0;
    left:0;
    height:320;
    width:100%;
    z-index:9999;
    background-color:#FFFFFF;
}
.select_work picker-view-column{
    text-align:center;
}
.select_work .work_button{
    font-size:14px;
    color:#919191;
    margin-top:6px;
    position:relative;
    padding:10px;
    display:inline-block;
    width:100%;
    font-weight:600;
}
.select_work .work_button span:last-child{
    float:right;
    margin-right:20px;
    color:#39c32b;
}
.point{
    background-image:url(data:image/png;base64,);
    width:10px;
    height:10px;
    display:inline-block;
    background-size:100%;
    background-repeat:no-repeat;

}
.doctor_modal{
    width:80%;
    position:fixed;
    background-color:#fff;
    z-index:99999;
    top:30%;
    left:10%;
    border-radius:5px;
    padding-bottom:10px;

}
.doctor_modal .doctor_modal_head{
    background-color:#00acff;
    width:100%;
    display:inline-block;
    padding:10px 0;
    text-align:center;
    color:#fff;
    font-size:16px;
    border-top-left-radius:5px;
    border-top-right-radius:5px;

}
.doctor_modal .doctor_modal_content{
    border-bottom:1px solid #e1e1e1;
    width:96%;
    display:inline-block;
    padding-left:10px;
    padding-top:10px;
    padding-bottom:5px;
    font-size:14px;
    color:#414b50;
    max-height:120px;
}
.doctor_modal .doctor_modal_content .checkbox{
    display:inline-block;
    width:100%;
    margin-bottom:5px;
}
.doctor_modal .doctor_modal_content .checkbox checkbox{
    float:left;
}
.doctor_modal .doctor_modal_content .checkbox p{
    overflow:hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
    width:65%;
    display:inline-block;
    float:left;
}
.doctor_modal .doctor_modal_content .checkbox span{
    float:right;
    margin-right:10px;

}
.doctor_modal .doctor_modal_footer{
    font-size:14px;
}
.doctor_modal .doctor_modal_footer checkbox{
    float:left;
    margin-left:10px;
    margin-top:10px;

}
.doctor_modal .doctor_modal_footer span{
    border-right:1px solid #e1e1e1;
    width:36px;
    display:inline-block;
    height:22px;
}
.doctor_modal .doctor_modal_footer a{
    font-size:16px;
    color:#00acff;
    font-weight:600;
    margin-left:28%;
    margin-top:10px;
    display:inline-block;

}
.weui-select_in-select-after {
    color:#414b50;

}
@media only screen and (min-width:320px) and (max-width:350px){
    .doctor_back[type="primary"] span{
        font-size:12px!important;
    }
}