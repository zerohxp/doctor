page {
    height:100%;
    background-color:#f5f5f5;
}
.page.has_bottom{
    padding-bottom:65px;
    margin-bottom:10px;

}
.page .weui-cell:before{
    border:0;
}
.doctor_button,button[type="primary"].doctor_button{
    width:90%;
    background-color:#00acff;
    color:#fff;
    border:0;
    font-weight:600;
}
button[loading][type="primary"].doctor_button{
    background-color:#00acff;
    color:#fff;
   
}


button[disabled][type="primary"].doctor_button {
    background-color:#00acff;
    opacity:0.4;
}
button[loading][type="primary"] {
    color:rgba(255, 255, 255, 0.6);
    background-color:#00acff;
    opacity:0.4;
}
button[disabled][type="primary"] {
    color:rgba(255, 255, 255, 0.6);
    background-color:#00acff;
    opacity:0.4;
}
.backdrop{
      width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background-color: rgba(0,0,0,.4);
    color: #fff;
    text-align: center;
    font-size: 16px;
    z-index:9999;

}

.page__footer{
    position:fixed;
    bottom:0;
    left:0;
    width:100%;
    display:inline-block;
    height:65px;
    background-color:#ffffff;
    border-top:1px solid #e1e1e1;
    z-index:999;

}
.page__footer .doctor_button{
    margin-top:10px;
    margin-bottom:0px;
}
.color_gray{
    color:#c5c5c5 !important;
}
.doctor_button .add{
    background-image:url(data:image/png;base64,);
   width:20px;
    height:20px;
    background-size:100%;
    display:inline-block;
    position:absolute;
    top:13px;

}
.doctor_button span{
    margin-left:25px;

}
.weui-label{
    width:100px;
}
.load_more{
    display:inline-block;
    text-align:center;
    width:100%;
    font-size:16px;
    color:#00acff;
    position:relative;

}
.load_more .down{
    width:0;
    height:0;
    border-left:6px solid transparent;
    border-right:6px solid transparent;
    border-top:10px solid #00acff;
    position:absolute;
    top:50%;
    margin-top:-5px;
    margin-left:5px;

}
.weui-input-placeholder{
    color:#bfbfbf;
    font-size:16px;
    z-index:1;
}
.weui-select-down:before{
    border-width:0px 2px 2px 0px;
}
.weui-cell_p .wrong{
    background-image:url(data:image/png;base64,);
    width:18px;
    height:16px;
    background-size:100%;
    position:absolute;
    left:0px;
    top:12px;

}

