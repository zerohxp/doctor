@import '../common/css/common.wxss';
.page__hd{
    background-color:#fff;
}
.weui-cell .user_image{
     width:55px;
    height:55px;
    border-radius:50%; 
    border:1px solid #0cb0ff;
    position:absolute;
    top:12px;
    left:50%;
    margin-left:-33px;

}
.page__hd .weui-cell:first-child{
    background-color:#fafafa;
    height:20px;

}
.base_info{
    font-size:10px;
    color:#414b50;
    display:inline-block;

}
.base_info .weui-cell__hd,.base_info .weui-cell__bd{
    width:66%;
    float:left;
}
.base_info .weui-cell__bd{
    width:34%;
}
.base_info .weui-cell__hd p,.base_info .weui-cell__bd p{
    display:inline-block;
    width:100%;
    position:relative;

}
.weui-cell i{
    width:12px;
    height:13px;
    background-size:100%;
    display:inline-block;
    background-repeat:no-repeat;
    margin-right:5px;
    position:absolute;
    left:-2px;
    top:0px;
}
.base_info .name,.base_info .dec,.base_info .tit,.base_info .email,.base_info .phone{
    padding-left:15px;
}
.base_info .name i{
    background-image:url(data:image/png;base64,);
}
.base_info .head{
    font-size:13px;
    color:#00acff;
    margin-right:10px;
}
.base_info .six{
    margin-left:10px;
    margin-top:5px;
}
.base_info .six span{
    margin-left:10px;
}
.base_info .dec i{
    background-image:url(data:image/png;base64,);
    height:15px;

}

.base_info .dec .head{
    font-size:11px;
    margin-right:2px;
}
.base_info .dec .block{
    background-color:#00acff;
    color:#fff;
    font-size:7px;
    padding:2px 5px;
    border-radius:15px;
    margin-left:5px;
}
.base_info .tit i{
    background-image:url(data:image/png;base64,);
    height:16px;
    top:-2px;

}
.base_info .tit span{
    margin-right:10px;
}
.base_info .email i{
    background-image:url(data:image/png;base64,);
    width:14px;
    height:10px;
    top:3px;
    left:-4px;

}
.base_info .phone i{
    background-image:url(data:image/png;base64,);
    height:16px;
    width:10px;

}
.base_info .dec,.base_info .email,.base_info .phone{
    margin-top:5px;
}
.base_info .tit{
    margin-top:2px;
}
.base_title{
    background-color:#fafafa;
    font-size:12px;
    color:#919191;
    display:block;
    width:100%;
    overflow:hidden;
    padding-left:0px;
    padding-right:0px;
    padding-bottom:0px;

}
.base_title.first{
    padding-top:10px;
}
.base_title:last-child{
    padding-bottom:20px;
}
.base_title .head{
    color:#00acff;
}
.base_title .weui-cell__hd{
    width:80px;
    display:inline-block;
    vertical-align:top;
    position:relative;
    padding-left:15px;
}
.base_title .weui-cell__bd{
    display:inline-block;
    width:70%;
}
.base_title .weui-cell__bd p{
    display:inline-block;
    width:100%;
   
}
.base_title .weui-cell__bd p span{
    margin-right:10px;
}
.base_title .tit1 i{
    background-image:url(data:image/png;base64,);
    top:2px;
    left:14px;

}
.base_title .tit2 i{
    background-image:url(data:image/png;base64,);
    top:3px;
    left:14px;

}
.base_title .tit3 i{
    background-image:url(data:image/png;base64,);
    top:2px;
    left:14px;
}
.base_title .tit1,.base_title .tit2,.base_title .tit3{
    padding-left:15px;
}
.base_title .icon{
    min-width:36px;
    width:auto;
    background-size:contain;
    background-repeat:no-repeat;
    padding-left:16px;
    margin-left:0px;
    font-size:10px;
    height:12px;
    margin-right:0!important; 
}
.base-block{
    margin-top:5px;
    background-color:#fff;
    font-size:10px;
    color:#414b50;
    display:block;
    
}
.base-block .head{
    font-size:12px;
    color:#00acff;
}
.base-block .tit1,.base-block .tit2,.base-block .tit3,.base-block .tit4,.base-block .tit5,.base-block .tit6 {
    position:relative;
    padding-left:15px;
}
.base-block .tit1 i{
    background-image:url(data:image/png;base64,);
    width:15px;
    top:-2px;

}
.base-block .tit2 i{
    background-image:url(data:image/png;base64,);
    width:19px;
    height:15px;
    left:-4px;
    top:-3px;

}
.base-block .tit3 i{
    background-image:url(data:image/png;base64,);
    width:15px;
    top:-3px;
    height:14px;

}
.base-block .tit4 i{
    background-image:url(data:image/png;base64,);
    width:15px;
    height:17px;
    top:-3px;

}
.base-block .tit5 i{
    background-image:url(data:image/png;base64,);
    width:15px;
    height:17px;
    top:-6px;

}
.base-block .tit6 i{
    background-image:url(data:image/png;base64,);
    width:16px;
    left:-3px;
    height:14px;
    top:-3px;

}
.base-block .weui-cell__bd{
    padding-left:15px;
    margin-right:30px;
}
.base-block span{
    margin-right:10px;
}
.base-block .long span{
    margin-right:6px;
}
.base-block span.dian{
    display:inline-block;
    background-color:#fd8925;
    width:5px;
    height:5.5px;
    border-radius:50%;
    margin-left:-10px;
    margin-right:6px;

}
.base-block .weui-cell__bd p{
    width:100%;
    display:block;

}
.base-block .content{
    color:#919191;
}
.mt5{
    margin-top:5px;
}
.ml20{
    margin-left:20px!important;
}
.bottom_icon{
    width:100%;
    display:block;
    text-align:center;
    color:#414b50;
    font-size:14px;
    position:relative;
    padding:10px 0px;

}
.bottom_icon i{
    background-image:url(data:image/png;base64,);
    width:20px;
    height:20px;
    display:inline-block;
    background-size:100%;
    background-repeat:no-repeat;
    position:absolute;
    margin-left:-20px;
    margin-top:1px;

}
button[type="primary"].doctor_button{
    width:44%;
    display:inline-block;

}
button[type="primary"].doctor_button:first-child{
    margin-left:5%;
    margin-right:2%;
}
button[type="primary"].doctor_button:last-child{
    margin-right:5%
}
.fs10{
    font-size:10px;
}
.doctor_modal .weui-cell{
   padding:2px 5px;
}
.doctor_modal .weui-cell::before {
    border:0;
}

.doctor_modal .weui-input{
    width:90%;
   
}
.doctor_modal .doctor_modal_footer a{
    width:49%;
    margin-left:0px;
    text-align:center;

} 
.doctor_modal .doctor_modal_footer a:first-child{
    border-right:1px solid #e1e1e1;
    color:#cdcdcd;

}
.weui-cell_p{
    color:#ff7a00;
    padding-right:0px;
    padding-top:0px;
    font-size:15px;
    padding-left:6px;
}
.weui-cell_p p{
    position:relative;
    padding-left:23px;
    padding-top:0px;
    letter-spacing:-0.5px;

}
.weui-cell_p .wrong{
    top:3px;
}

/**/
.pickerModal{
    position:fixed;
    bottom:0;
    width:100%;
    z-index:99999;
    background-color:#fff;

}
.pickerModal .pickerModal_btn{
    font-size:14px;
    background-color:#f5f5f5;
    text-align:center;
    padding-top:5px;
    padding-bottom:5px;

}
.pickerModal .pickerModal_btn span{
    color:#414b50;
}
.pickerModal .pickerModal_btn .left{
    float:left;
    margin-left:15px;
    color:#919191;

}
.pickerModal .pickerModal_btn .right{
    float:right;
    margin-right:15px;
    color:#00acff;

}
.pickerModal .checkbox{
    width:100%;
    display:block;
    border-bottom:1px solid #e1e1e1;
    padding-top:5px;
    padding-bottom:5px;
    padding-left:44%;
    font-size:15px;

}