@import '../common/css/common.wxss';
page{
    background-color:#fff;
}
.before_login_p{
    width:100%;
    padding:5px 20px;
    display:inline-block;

}
.page__hd .weui-cell{
    border-bottom:1px solid #e1e1e1;
}

.weui-cell_image span{
   line-height:60px;
    float:left;
    font-size:16px;
    margin-left:10px;
    color:#414b50;
    font-weight:600;
}
.weui-cell_image image{
    width:55px;
    height:55px;
    border-radius:50%;
    border:1px solid #0cb0ff;
   float:left;
}
.login_user_info p{
    font-size:15px;
    margin-left:10px;
}
.login_user_right{
   float:right;
    width:58px;
    text-align:right;
    font-size:12px;
    color:#919191;
    margin-right:5px;
}
.login_user_right span{
    width:100%;
    display:inline-block;
    white-space: nowrap;
}
.login_user_right span:last-child{
   color:#fd8925;
}
.weui-cell__ft_in-access:after{
    border-color:#00acff;
}
.login_top_bottom{
   font-size:14px;
    padding:5px 20px;
    display:flex;

}
 .login_center_block{
    text-align:center;
    width:100%;
    display:inline-block;
    white-space: nowrap;
    padding:10px 0;
    border-bottom:1px solid #e1e1e1;
    font-size:14px;
    color:#00acff;
    font-weight:600;
}
.login_center_block span{
    display:inline-block;
    width:33%;
    border-right:1px solid #919191;

}
.login_center_block span:last-child{
    border:0;
}
.login_center_flag{
    width:100%;
    display:inline-block;
    font-size:16px;
    background-color:#77d2fd;
   padding:8px 0px 8px 0px;
    position: relative;

}
.login_center_flag i{
    background-image:url(data:image/png;base64,);
    width:15px;
    height:22px;
    display:inline-block;
    background-size:100%;
    position:absolute;
    left:15px;
    background-repeat:no-repeat;
    top:10px;
}
.login_center_flag .flag_tit{
    color:#fff;
    margin-right:5px;
    margin-left:32px;
}
 .login_center_flag .flag_span{
    font-size:12px;
    color:#fd8925;
    margin-left:8px;
    background-color:#fff;
    border-radius:15px;
    padding:3px 5px;

}
.page_profession{
    display:inline-block;
    width:100%;
    text-align:center;
position: relative;
padding-bottom:40px;
padding-top:20px;
     background: -webkit-linear-gradient(top, #01acff 0%, #75d1fd 100%);
    background: -ms-linear-gradient(top, #01acff 0%, #75d1fd 100%);
    background: -moz-linear-gradient(top, #01acff 0%, #75d1fd 100%);
    background: linear-gradient(top, #01acff 0%, #75d1fd 100%);
}
.page_profession image{
    width:320px;
    height:280px;
    z-index:999;
    top:50%;
    left:50%;
    position:absolute;
    margin-left:-160px;
    margin-top:-96px;


}
.page_profession .score{
    z-index:99999;
   top:55%;
    left:50%;
    position:absolute;
    font-size:30px;
    color:#fff;
    font-weight:600;
    margin-left:-20px;
    font-family:微软雅黑;

}
.profession_develop{
    margin-top:20px;
    text-align:center;
    font-weight:600;
    padding:0;
    color:#fff;
}

.profession_develop_tit{
     text-align:center;
    padding:0;
    color:#fff;
    font-size:14px;
}
canvas{
    width: 320px;
    height: 220px;
    margin-top:35px;
    position:relative;
    left:50%;
    margin-left:-160px;
    z-index:999;

}
.page.has_bottom{
    margin-bottom:0px;
}

button[type="primary"].doctor_button{
    width:58%;
    display:inline-block;
    float:right;
    right:5%;
}
.doctor_back[type="primary"]
{
    font-weight:600;
    width:30%;
    background-color:#77d2fd;
    margin-top:10px;
    display:inline-block;
    left:5%;
    height:46px;
    padding:0px;
}
@media only screen and (min-width:320px) and (max-width:350px){
     .login_center_flag .flag_span{
          font-size:13px;
        margin-left:5px;
     }
     .page_profession image{
         margin-left:-160px;
        margin-top:-106px;

     }
     .page_profession{
         padding-top:0px;
         padding-bottom:30px;
     }
     .page_profession image{
         margin-top:-99px;
     }
}
@media only screen and (min-width:400px) and (max-width:414px){
    .page_profession{
         padding-bottom:85px;
        padding-top:40px;

     }
     .page_profession image{
         margin-top:-103px;
     }
     .page_profession .score{
         top:53%;
     }
}

