@import '../common/css/common.wxss';

button[type="primary"].doctor_button{
    width:60%;
    display:inline-block;
    float:right;
    right:5%;
}
.doctor_back[type="primary"]
{
    font-weight:600;
    width:28%;
    background-color:#77d2fd;
    margin-top:10px;
    display:inline-block;
    left:5%;
    height:46px;
}
.weui-cell{
    background-color:#fff;
    border-bottom:1px solid #e1e1e1;
    margin-top:0px;
    font-size:16px;
    color:#414b50;
}
.weui-cells__title{
    background-color:#fff;
    margin-bottom:0;
    margin-top:5px;
    border-top:1px solid #e1e1e1;
     font-size:16px;
    color:#414b50;
    font-weight:600;
    padding-top:10px;

}
.weui-cells__title span{
     color:#fd8925;
    margin-left:5px;
}
.weui-label{
    font-weight:600;
}
.weui-cell_textarea{
    padding:10px 0px;
}
.weui-cell_textarea textarea{
    width:100%;
    padding:10px 15px;
    background-color:#fafafa;
}
.weui-cell_p{
    color:#ff7a00;
    padding-right:0px;
    padding-top:0px;
    font-size:14px;

}
.weui-cell_p p{
    position:relative;
    padding-left:20px;
    border-top:1px solid #e1e1e1;
    padding-top:10px;
    letter-spacing:-0.5px;

}
.weui-cell_p .wrong{
    background-image:url(data:image/png;base64,);
    width:18px;
    height:16px;
    background-size:100%;
    position:absolute;
    left:0px;
    top:12px;

}
.no_bottom{
    border:0;
}
@media only screen and (min-width:320px) and (max-width:350px){
    .weui-cell_textarea textarea{
        height:100px;
    }
    .weui-cell_p{
        font-size:12px;
    }
}