}
.doctor_back[type="primary"]
{
    font-weight:600;
    width:80%;
    background-color:#fff;
    border:1px solid #50BE8A;
    color:#50BE8A;
    margin-top:10px;
}
.weui-cells__title{
    padding:10px 15px 0px;
}
.weui-cell_input{
    padding-top:0px;
   
}
.weui-cell_input .weui-input{
     width:100%;
     background-color:#F2F2F2;
    border-radius:5px;
    padding-left:5px;

}
.weui-cell_image .default{
    width:80px;
    height:80px;
    border:1px solid #D7D7D7;
    border-radius:5px;

}
.weui-cell_image{
    border:0;
}
.weui-cell_image .upload{
    width:100%;
    height:200px;

}
