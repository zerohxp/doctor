@import 'common.wxss';

.content_block table{
    font-size:15px;
    margin-left:15px;
    background-color:#e3f6ff;
    color:#8b99a0;
    display: table;
    border-collapse:collapse;
    margin-top:5px;
    margin-bottom:10px;

}
.content_block table thead{
        display: table-header-group;
    vertical-align: middle;
    border-color: inherit;
}
.content_block table tbody{
        display: table-row-group;
    vertical-align: middle;
    border-color: inherit;
}
.content_block table tr {
    display: table-row;
    vertical-align: inherit;
    border-color: inherit;
     height:40px;
     line-height:40px;
     border-bottom:1px solid #fff;
}
.content_block table tbody tr{
    height:50px;
    line-height:50px;
}
.content_block table td {
    display: table-cell;
    vertical-align: inherit;
    text-align:center;
    border-right:1px solid #fff;
    position:relative;

}
.calendar_table .no_appoint{
    height:100%;
    width:0px;
    display:block;
    background-color:#80daa8;
    position:absolute;
    bottom:0;
    left:0;
}

.calendar_table .appoint{
    height:100%;
    width:0px;
    display:block;
    background-color:#ffce96;
    position:absolute;
    bottom:0;
    left:0;
}
.calendar_table .free{
    height:100%;
    width:0px;
    display:block;
    background-color:#73d1ff;
    position:absolute;
    bottom:0;
    left:0;
}
.calendar_table .no_appoint,.calendar_table .appoint,.calendar_table .free{
    font-size:14px;
    color:#414b50;
    overflow:hidden;
    text-overflow:ellipsis;
    display:-webkit-box;
    -webkit-line-clamp:3;
    -webkit-box-orient:vertical;
    line-height:16px;
    text-align:left;
    font-weight:600;
    z-index:99999;
}
.doctor_modal .doctor_modal_content{
    border-bottom:0;
    height:70%;
    max-height:70%;
}
.doctor_modal .doctor_modal_content .title{
    color:#8dd0ff;
}
.doctor_modal .doctor_modal_content .content{
    color:#414b50;
}
.doctor_modal .doctor_modal_content .content span{
    margin-right:10px;
}
.doctor_modal .doctor_modal_content .timer{
    color:#9ea2a4;
}
.doctor_modal{
    top:15%;
}
.doctor_modal_head .close{
    background-image:url(data:image/png;base64,);
    width:22px;
    height:21px;
    display:block;
    background-size:100%;
    position:absolute;
    top:13px;
    right:10px;

}
button[type="primary"].left_btn{
    margin-left:1%;
    margin-right:0.5%;
    width:49%;
}
