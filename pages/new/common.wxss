page,.page{
    height:100%;
    background-color:#f5f5f5;
    font-family:思源黑体;
}
.textarea-placeholder{
    color:#bfbfbf;
    font-size:15px;
    z-index:1;
    font-weight:normal;
}
.weui-input-placeholder{
    color:#bfbfbf;
    font-size:15px;
}
button[type="primary"].primary_btn{
    background-color:#00acff;
    width:90%;
    margin:10px 5%;

}
.tab_head{
    width:50%;
    margin-left:25%;
    border:1px solid #00acff;
    border-radius:5px;
    height:40px;
    line-height:40px;
    font-size:16px;
    margin-top:5px;
    margin-bottom:5px;
    color:#414b50;
    font-weight:600;
    font-family:思源黑体;
    background-color:#f5f5f5;
}
.tab_head .tab_head_title{
    float:left;
    width:50%;
    text-align:center;
    
}
.tab_head .tab_head_title.active{
    background-color:#00acff;
    color:#fff;
}
.tab_content{
    display:none;
   

}
.tab_content.show{
    display:block;
}

.content_block{
    padding:0px;
    width:100%;
    background-color:#fff;
    margin-bottom:5px;
    border-top:1px solid #e1e1e1;
    border-bottom:1px solid #e1e1e1;
}
.weui-select{
    border-right:0px;
    padding-left:0px;
    text-align:right;
    padding-right:15px;

}
.weui-select::before{
    right:0px;
}
.weui-select-down:before{
    border-width:0px 2px 2px 0px;
}
button[type="primary"].left_btn{
    width:44%;
    font-size:16px;
    float:left;
    margin-left:5%;
    margin-right:2%;
    background-color:#48c2fc;
}
button[type="primary"].right_btn{
    width:44%;
    font-size:16px;
    margin-right:5%;

}
.footer{
    position:fixed;
    bottom:0;
    left:0;
    width:100%;
    display:inline-block;
    background-color:#ffffff;
    border-top:1px solid #e1e1e1;
    z-index:999;
}
weui-cell::before{
    border-top:1rpx solid #e1e1e1;
}
button{
    font-size:16px;
}
.backdrop{
      width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background-color: rgba(0,0,0,.4);
    color: #fff;
    text-align: center;
    font-size: 16px;
    z-index:9999;

}
.doctor_modal{
    width:80%;
    position:fixed;
    background-color:#fff;
    z-index:99999;
    top:30%;
    left:10%;
    border-radius:5px;
    padding-bottom:10px;

}
.doctor_modal .doctor_modal_head{
    background-color:#00acff;
    width:100%;
    display:inline-block;
    padding:10px 0;
    text-align:center;
    color:#fff;
    font-size:16px;
    border-top-left-radius:5px;
    border-top-right-radius:5px;

}
.doctor_modal .doctor_modal_content{
    border-bottom:1px solid #e1e1e1;
    width:96%;
    display:inline-block;
    padding-left:10px;
    padding-top:10px;
    padding-bottom:5px;
    font-size:14px;
    color:#414b50;
    max-height:120px;
}
.home{
    position:fixed;
    background-color:#ffa640;
    color:#fff;
    width:40px;
    height:40px;
    font-size:14px;
    border-radius:50%;
    right:10px;
    bottom:50px;
    z-index:9999;

}
.home i{
    background-image:url(data:image/png;base64,);
    width:20px;
    height:20px;
    background-size:100%;
    display:block;
    position:absolute;
    left:50%;
    margin-left:-10px;

}
.home span{
    top:18px;
    position:absolute;
    left:5px;
}