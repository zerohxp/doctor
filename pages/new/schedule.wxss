@import 'common.wxss';

.title{
    color:#919191;
    font-size:14px;
    margin-left:15px;
    margin-top:10px;
    margin-bottom:5px;
}
.content_block{
    color:#414b50;
    font-size:15px;
    font-weight:600;
}
.content_block .tab_head_title{
    border:1px solid #414b50;
    color:#414b50;
    border-radius:5px;
    width:100px;
    display:inline-block;
    height:35px;
    line-height:35px;
    margin-bottom:15px;
    margin-left:15px;
    text-align:center;
    margin-top:5px;

}
.content_block .tab_head_title.active{
    border:1px solid #00acff;
    color:#00acff;
    position: relative;
}
.content_block .tab_head_title.active i{
    background-image:url(data:image/png;base64,);
    background-size:100%;
    width:14px;
    height:13px;
    display:block;
    position:absolute;
    right:0px;
    bottom:0px;
    background-repeat:no-repeat;
}
.content_block .weui-label{
    width:70px;
}
.content_block .weui-label span{
    color:#fd8925;
    margin-left:10px;
}

.content_time_block picker{
    width:32%;
    float:left;

}
.content_time_block picker .weui-select{
    text-align:left;

}
.content_time_block .weui-select::before{
    right:15px;
}
.content_time_block .add{
    background-image:url(data:image/png;base64,);
    background-size:100%;
    width:20px;
    height:20px;
    display:block;
    position:absolute;
    right:8px;
    top:50%;
    margin-top:-8px;
}
.content_time_block .del{
    background-image:url(data:image/png;base64,);
    background-size:100%;
    width:21px;
    height:20px;
    display:block;
    position:absolute;
    right:8px;
    top:50%;
    margin-top:-8px;
}
button[type="primary"].left_btn{
    font-size:14px;
}
button[type="primary"].right_btn{ 
     font-size:14px;
}