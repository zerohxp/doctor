@import 'common.wxss';
.content_block:first-child{
    margin-top:5px;
}
.record_block{
    padding-top:10px;
    padding-bottom:5px;
    background-color:#e5f7ff;
}
.record_block .title{
    margin-left:15px;
    color:#00acff;
    font-size:16px;
    font-weight:600;

}
.record_block .content{
    margin-left:15px;
    margin-top:10px;

}
.record_block .content_title{
    float:left;
    margin-right:20px;
    color:#414b50;
    font-size:15px;
    font-weight:600;

}
.record_block .content_record{
    font-size:15px;
    color:#919ea4;
    margin-left:74px;

}
.record_block .content_record span{
    margin-right:10px;

}
.record_block i{
    width:0;
    height:0;
    border-left:10px solid transparent;
    border-right:10px solid transparent;
    border-top:20px solid #e5f7ff;
    position:absolute;
    left:15px;

}
.content_block  .record_block_edit{
    margin-left:15px;
    border-bottom:1px solid #e1e1e1;
    padding-top:5px;
    padding-bottom:5px;
}
.content_block .record_block_edit.first{
    padding-top:18px;
}
.content_block .record_block_edit:last-child{
    border-bottom:0;
}

.record_block_edit .title{
    color:#00acff;
    font-size:16px;
    font-weight:500;
    position: relative;
}
.record_block_edit .title span{
    margin-right:20px;
}

.record_block_edit .title .edit{
    background-image:url(data:image/png;base64,);
    width:17px;
    height:16px;
    background-size:100%;
    display:block;
    top:50%;
    position:absolute;
    margin-top:-9px;
    right:15px;
    background-repeat:no-repeat;
}
.record_block_edit .record_title{
    font-size:15px;
    color:#414b50;
    font-weight:600;

}
.record_block_edit .record_desc{
    font-size:15px;
    color:#9ea2a4;
    font-weight:600;

}
.filter{
    position:fixed;
    background-color:#00acff;
    color:#fff;
    font-size:14px;
    padding:2px 10px;
    border-radius:5px;
    top:10px;
    right:15px;
    padding-left:30px;
}
.filter i{
    background-image:url(data:image/png;base64,);
    width:15px;
    height:15px;
    background-size:100%;
    background-repeat:no-repeat;
    display:block;
    position:absolute;
    left:10px;
    top:50%;
    margin-top:-8px;

}
.filterPicker{
    position:fixed;
    width:100px;
    height:30px;
    top:10px;
    right:15px;
    z-index:9999;
    opacity:0;

}