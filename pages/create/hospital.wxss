@import '../common/css/common.wxss';
page{
    height:100%;
    background-color:#f4f4f4;
}
.page{
    background-color:#ffffff;
}
.page__hd{
    padding-top:15px;
    padding-bottom:15px;
    border-bottom:1px solid #e5e5e5;
}
.page__hd .weui-input{
    border:1px solid #e5e5e5;
    border-radius:5px;
    padding-left:10px;
    background-color:#f4f4f4;
    font-size:16px;
    color:#a8a8a8;
}
.page__bd .bd_p{
    margin-top:40%;
}
.page__bd .bd_p p{
    color:#666666;
    width:100%;
    display:block;
    text-align:center;
    font-weight:600;
    font-size:22px;
}
.page__bd .weui-cell{
    border-bottom:1px solid #e5e5e5;
}
.page__bd .weui-cell .weui-cell__bd{
    line-height:18px;
    padding-left:10px;
}
.page__bd .weui-cell .weui-cell__bd p{
    width:100%;
    display:inline-block;
    font-size:16px;
    line-height:11px;
    font-weight:600;
    color:#414b50;
}
.page__bd .weui-cell .weui-cell__bd p:last-child{
    font-weight:normal;
    font-size:12px;
    color:#919191;
}