@import '../common/css/common.wxss';

.page__hd{
   background-color:#00acff;
    color:#fff;
    padding-top:10px;
    text-align:center;
}
.page__hd image{
    width:100%;
    height:80px;

}
.page__hd p{
    font-size:16px;
}
.page__bd{
    padding:20px 15px;
    font-size:16px;
}
.page__bd .doctor_p{
    margin-bottom:20px;
    display:block;
    color:#414b50;
    font-size:14px;
    font-weight:600;

}
.page__bd .doctor_p span{
    color:#00a9ff;
margin-left:40px;

}
.doctor_choose_block{
    border:1px solid #b6e3f9;
border-radius:10px;
padding:10px 20px;
margin-bottom:15px;
background-color:#b6e3f9;
color:#414b50;
font-weight:600;
font-size:15px;

}
.doctor_choose_block.disabled{
    background-color:#dfdfdf;
color:#afafaf;
border:0;

}
.doctor_choose_block.disabled .right{
   margin-left:50px;
font-size:12px;

}
.doctor_choose_block.active{
    color:#fff;
    border:1px solid #48c2fc;
    background-color:#48c2fc;

}
.doctor_a{
    text-decoration: underline;
    color:#50BE8A;
    font-size:16px;
    text-align:center;
    display:block;
     margin-top :20px;
}
.doctor_button{
    margin-top :80px;

}
.next_a{
    font-size:14px;
    color:#00acff;
    font-weight:600;
}
.next_a:before{
    border-color:#00acff;
}
.doctor_choose_block i{
    width:20px;
    height:20px;
    display:inline-block;
    background-size:100%;
    background-repeat:no-repeat;
    margin-right:10px;

}
.doctor_type_1 i{
    background-image:url(data:image/png;base64,);
    height:21px;
}
.doctor_type_1.active i{
    background-image:url(data:image/png;base64,);
}
.doctor_type_2 i{
    background-image:url(data:image/png;base64,);
}
.doctor_type_2.active i{
    background-image:url(data:image/png;base64,);
}
.doctor_type_3 i{
    background-image:url(data:image/png;base64,);
}
.doctor_type_3.active i{
    background-image:url(data:image/png;base64,);
}
.doctor_type_4 i{
    background-image:url(data:image/png;base64,);
    width:26px;
    margin-right:6px;
}
@media only screen and (min-width:320px) and (max-width:350px){
    .page__hd image{
        height:70px;
    }
}
@media only screen and (min-width:380px) and (max-width:414px){
    .page__hd image{
        height:94px;
    }
}
