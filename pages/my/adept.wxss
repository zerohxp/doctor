@import '../common/css/common.wxss';

.page__hd{
    padding:12px 10px;
    background-color:#00acff;

}
.page__bd{
    border-top:1px solid #e1e1e1;
    margin-top:10px;
}
.page__hd .weui-cell__hd{
    width:82%;
}
.page__hd .weui-cell_input{
    padding:0;
}
.page__hd .weui-cell__hd .weui-input{
    background-color:#fff;
    border-radius:5px;
    height:40px;
    padding-left:5px;
    font-size:15px;
    line-height:40px;
    border-top-right-radius:0px;
    border-bottom-right-radius:0px;

}
.page__hd .weui-cell__bd .doctor_button{
    width:100%;
    border-top-left-radius:0px;
    border-bottom-left-radius:0px;
    height:41px;
    line-height:41px;
    font-size:15px;
    background-color:#fdb72b;
    border:0;
}
.page__hd .weui-cell__bd .doctor_button:after{
    border:0;
}
.page__hd p{
    font-size:15px;
    color:#fff;
    margin-top:8px;
    display:inline-block;

}
.page__bd .weui-cell{
    background-color:#fff;
    font-size:16px;
    border-bottom:1px solid #e1e1e1;
}

.page__bd .weui-cell .weui-cell__hd{
    width:90%;
}
.page__bd .weui-cell .weui-label{
    width:100%;
    color:#414b50;

}
.weui-cell__bd .close{
    background-image:url(data:image/png;base64,);
    width:20px;
    height:20px;
    background-size:100%;
    display:inline-block;
    position:absolute;
    top:50%;
    margin-top:-10px;
    right:10px;
}