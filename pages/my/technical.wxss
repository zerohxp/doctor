@import '../common/css/common.wxss';

.weui-cell{
    background-color:#fff;
    font-size:16px;
    padding-top:5px;
    padding-bottom:5px;
    margin-bottom:5px;
    margin-top:5px;
    border-top:1px solid #e1e1e1;
     border-bottom:1px solid #e1e1e1;
      color:#414b50;
}
.weui-cell__bd{
    width:100%;
}
.weui-cell .weui-select_in-select-after{
    text-align:right;
    padding-right:35px;
    border:0;
}
.weui-label{
    font-weight:600;
}
.next_a{
    font-size:15px;
    color:#00acff;
    border:0;
    padding-right:25px;
    position:absolute;
    bottom:100px;
    right:2px;
}
.next_a:before{
    border-color:#00acff;
}