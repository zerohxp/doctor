@import '../common/css/common.wxss';
page{
    height:100%;
    background-color:#EEECED;
}
.weui-cell{
    background-color:#fff;
    margin-bottom:10px;
    padding:20px 15px;
}
.weui-cell .weui-cell_p{
    font-size:14px;
    color:#666666;
}
.weui-cell .weui-cell_p span{
    font-size:20px;
    color:#333;
    margin-right:15px;
}
.weui-cells__title{
    background-color:#fff;
    margin-bottom:0px;
    margin-top:0px;
    padding-top:10px;
    color:#666;
}
navigator.weui-cell{
    padding-top:5px;
    font-size:16px;
    padding-bottom:10px;

}
navigator.weui-cell .weui-cell__bd{
    text-align:right;
    font-size:12px;
    color:#999999;
}
navigator.weui-cell-block{
    margin-bottom:0px;
    padding-bottom:5px;
}