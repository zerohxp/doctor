@import '../common/css/common.wxss';
page{
    height:100%;
    background-color:#EEECED;
}
.weui-cell{
    background-color:#fff;
    font-size:16px;
    padding-top:5px;
    padding-bottom:5px;
    margin-bottom:15px;
}
.weui-cell__bd{
    width:100%;
}
.weui-cell .weui-select_in-select-after{
    text-align:right;
    padding-right:35px;
    border:0;
}
.doctor_button{
    bottom:12%;
    position:absolute;
    margin-left:10%;
}
.next_a{
    font-size:15px;
    color:#50BE8E;
    margin-bottom:25px;
    position:absolute;
    bottom:0;
    right:15px;

}
.next_a span{
    color:#9DA2A5;
}
.page__hd .weui-cell{
    margin-bottom:0px;
}
.page__hd p{
    font-size:14px;
    letter-spacing:-1px;
    padding-left:5px;
    color:#666666;

}
.page__bd{
    margin-top:5px;
}
.page__bd p.title{
    background-color:#fff;
    display:inline-block;
    width:96%;
    font-size:16px;
    color:#666666;
    padding:20px 0px 0px 15px;

}
.page__bd p.title span:last-child{
    float:right;
}
.page__bd .weui-cell_block{
    margin-bottom:0px;
    padding-bottom:0px;
    margin-top:-2px;
    font-size:15px;
}
.page__bd .weui-cell_block.weui-cell_block_last{
    margin-bottom:15px; 
}
.weui-label{
    width:130px;
}