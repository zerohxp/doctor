@import '../common/css/common.wxss';

button[type="primary"].doctor_button{
    width:58%;
    display:inline-block;
    float:right;
    right:5%;
}
.doctor_back[type="primary"]
{
    font-weight:600;
    width:30%;
    background-color:#77d2fd;
    margin-top:10px;
    display:inline-block;
    left:5%;
    height:46px;
    padding:0px;
    line-height:20px;
    font-size:16px;

}
.doctor_back[type="primary"] span{
    font-size:14px;
    display:inline-block;
    width:100%;
    white-space:nowrap;
    padding:0;

}
.weui-cell{
    background-color:#fff;
    border-bottom:1px solid #e1e1e1;
    border-top:1px solid #e1e1e1;
    margin-top:5px;
}
.weui-cell .weui-cell__hd image{
    width:100px;
    height:100px;
}
.weui-cell__bd{
    padding-left:15px;
    margin-top:-10px;
    width:66%;
}
.weui-cell__bd p{
    display:inline-block;
    width:100%;
    font-size:16px;
}
.weui-cell__bd p.title{
    font-weight:600;
    color:#414b50;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.weui-cell__bd p.content{
    color:#919191;
    font-size:14px;
    line-height:18px;
    height:35px;
    text-overflow:ellipsis;
    overflow:hidden;
    display:-webkit-box;
    -webkit-line-clamp:2;
    -webkit-box-orient:vertical;
    word-break:break-all;

}
.weui-cell__bd p.time{
    padding-left:26px;
    position:relative;
    color:#414b50;
    margin-top:8px;

}
.weui-cell__bd p.time i{
    background-image:url(data:image/png;base64,);
    width:24px;
    height:24px;
    background-size:100%;
    display:inline-block;
    position:absolute;
    left:0;
    top:1px;

}
@media only screen and (min-width:320px) and (max-width:350px){
    .weui-cell .weui-cell__hd image{
        width:80px;
        height:80px;
    }
    .weui-cell__bd{
        margin-top:0px;
    }
    .weui-cell__bd p.time{
        margin-top:0px;
    }
}